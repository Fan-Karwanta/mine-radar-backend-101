# Server Configuration
PORT=3000

# Database Configuration
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=AppName

# Authentication
JWT_SECRET=your-super-secret-jwt-key-here

# Cloudinary Configuration (for image uploads)
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Google Maps API
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Environment
NODE_ENV=production

# API URLs
API_URL=https://your-backend-domain.vercel.app

# CORS Origins - Frontend URLs that can access this API
FRONTEND_URL=https://your-admin-panel-domain.vercel.app
CORS_ORIGINS=https://your-admin-panel-domain.vercel.app,http://localhost:5173,http://localhost:3001

# Session/Cookie settings for production
COOKIE_DOMAIN=.vercel.app
SECURE_COOKIES=true
